<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity2">

    <TextView
        android:id="@+id/tituloActivity2"
        android:layout_width="335dp"
        android:layout_height="76dp"
        android:layout_marginTop="50dp"
        android:gravity="center"
        android:text="Bienvenido al Activity 2!!!"
        android:textSize="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/ir_sitio_web"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="76dp"
        android:text="Ir al Sitio Web"
        app:layout_constraintEnd_toEndOf="@+id/tituloActivity2"
        app:layout_constraintStart_toStartOf="@+id/tituloActivity2"
        app:layout_constraintTop_toBottomOf="@+id/tituloActivity2" />

    <Button
        android:id="@+id/volver"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="Volver"
        app:layout_constraintEnd_toEndOf="@+id/ir_sitio_web"
        app:layout_constraintStart_toStartOf="@+id/ir_sitio_web"
        app:layout_constraintTop_toBottomOf="@+id/ir_sitio_web" />
</androidx.constraintlayout.widget.ConstraintLayout>